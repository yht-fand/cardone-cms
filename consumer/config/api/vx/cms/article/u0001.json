{
  "validation": {
    "rules": {
      "articleId": {
        "required": true
      },
      "name": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      }
    },
    "messages": {
      "articleId": {
        "required": "文章标识必填"
      },
      "name": {
        "required": "文章名称必填",
        "minlength": "文章名称需{0}个字符以上",
        "maxlength": "文章名称需小于{0}个字符"
      }
    }
  }
}
<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <!-- 文章 -->
    <bean name="top.cardone.cms.service.ArticleService" parent="consumerParent">
        <property name="serviceInterface" value="top.cardone.cms.service.ArticleService"/>
        <property name="queue">
            <bean class="org.apache.activemq.command.ActiveMQQueue">
                <constructor-arg value="cardone-cms-${cardone-cms-version}:top.cardone.cms.service.ArticleService"/>
            </bean>
        </property>
    </bean>
</beans>